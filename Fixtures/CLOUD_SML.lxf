{
 "label": "CLOUD_SMALL",
 "tags": ["SMALL"],
 "parameters": {
   "hostParam": {
       "type": "string", "default": "127.0.0.1",
       "label": "Host",
       "description": "Hostname or IP address for output"
     }
  },
  "components": [
    { "type": "strip", "x": 0 , "y": 0, "z": 0, "numPoints": 30, "spacing": 0.656, "direction": { "x": 1, "y": 0, "z": 0 }, "tags": ["UPPER"] },
    { "type": "strip", "x": 0 , "y": 0, "z": -2, "numPoints": 30, "spacing": 0.656, "direction": { "x": 1, "y": 0, "z": 0 }, "tags": ["LOWER"] }
  ],
  "outputs": [
    {"protocol": "artnet", "universe": 0, "host": "${hostParam}", "start": 0, "num": 30, "byteOrder": "rgb"},
    {"protocol": "artnet", "universe": 1, "host": "${hostParam}", "start": 30, "num": 30, "byteOrder": "rgb"}
  ]
}
